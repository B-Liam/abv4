//this code gets the gallery photos for the vegetable
let vegPhotos = JSON.parse(`{{ gallery | getPlantPhotos( pagedata.content.title | lower ) | dump | safe }}`);

function initialise(containerid, templateid, divid) {
 
 const container = document.querySelector(containerid);
 const template = document.querySelector(templateid);

for ( let i = 0 ; i < vegPhotos[0].photos.length ; i++ ) {
    const element = template.content.cloneNode(true);
    const image = element.querySelector('img');
    image.src = `https://res.cloudinary.com/allotment-book/image/upload/l_text:Cherry Swash_25:Â©%20AllotmentBook.co.uk,g_south_west,x_20,y_20,co_white/w_600,h_600,c_scale,q_auto,f_auto/${vegPhotos[0].photos[i].url}`;
    image.alt = vegPhotos[0].photos[i].alt;
    element.getElementById(divid).innerHTML = vegPhotos[0].photos[i].alt;
    container.appendChild(element);
}
}

initialise(".gallery-container",'#gallery-template',"gallery-link");


